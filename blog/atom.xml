---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">Jake's Ramblings</title>
    <link type="application/atom+xml" href="http://jhartz.github.com/blog/atom.xml" rel="self"/>
    <link type="text" href="http://jhartz.github.com/blog/" rel="alternate"/>
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://jhartz.github.com/blog/</id>
    <author>
        <name>Jake Hartz</name>
    </author>
    {% for post in site.posts limit:20 %}
    <entry>
        <title>{{ post.title | escape }}</title>
        <link href="http://jhartz.github.com{{ post.url | escape }}"/>
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <id>http://jhartz.github.com{{ post.url | escape }}</id>
        <summary type="html">{{ post.excerpt | escape }}</summary>
    </entry>
    {% endfor %}
</feed>
