---
---
<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">
    <title type="text" xml:lang="en">Ramblings by Jake</title>
    <link rel="self" type="application/atom+xml" href="http://jhartz.github.com/blog/atom.xml" />
    <link rel="alternate" type="text/html" href="http://jhartz.github.com/blog/" />
    <updated>{{ site.time | date_to_xmlschema }}</updated>
    <id>http://jhartz.github.com/blog/</id>
    <author>
        <name>Jake Hartz</name>
    </author>
{% for post in site.posts limit:20 %}
    <entry>
        <id>http://jhartz.github.com{{ post.url | xml_escape }}</id>
        <title>{{ post.title | xml_escape }}</title>
        <link rel="alternate" type="text/html" href="http://jhartz.github.com{{ post.url | xml_escape }}" />
        <updated>{{ post.date | date_to_xmlschema }}</updated>
        <summary type="html">{{ post.excerpt | xml_escape }}</summary>
        <content type="html">{{ post.content | xml_escape }}</content>
    </entry>{% endfor %}
</feed>
